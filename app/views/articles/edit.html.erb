<h2>Edit article</h2>

<%= form_for :article, url: {action: "update"}, method: :put do |form| %>
  <p>
    <%# <span>Title: </span><br> %>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </p>
  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>
  <%= form.submit("Save") %>
<% end %>


 